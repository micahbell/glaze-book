.row
  .medium-10.columns
    h1 Mid Range Glazes
  .medium-2.columns
    a(href='/glazes') ADD

for val in midRecipe
  .row
    .medium-2.columns
      a(href='/glazes/' + val._id) IMG
    .medium-9.columns
      a(href='/glazes/' + val._id) #{val.title}
      p
        | #{val.tempRange} 
        span.pipe &nbsp;|&nbsp; 
        |  #{val.cone} 
        span.pipe &nbsp;|&nbsp; 
        |  #{val.surface}
    .medium-1.columns
      p Fav
